{
    "presets": [
      "react",
      ["es2015", { "modules": false }],
      "es2016",
      "es2017",
      "stage-2"
    ],
    "plugins": [
      "transform-export-extensions"
    ],
    "env": {
      "production": {
        "plugins": [
          "minify-dead-code-elimination",
          "transform-inline-consecutive-adds",
          "transform-react-remove-prop-types"
        ]
      },
      "development": {
        "plugins": [
          "react-hot-loader/babel",
          "styled-name"
        ]
      }
    }
  }