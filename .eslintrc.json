{
    "extends": ["atomix-react"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js"
          ],
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    }
  }